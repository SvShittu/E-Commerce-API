const{isTokenValid} = require("../utils")


const authenticateUser = async(req, res, next)=>{
const token = req.signedCookies.token
if(!token){
    console.log("error, no token present")
} 

try {
    const payload = isTokenValid({token})
    console.log(payload)
    next()
} catch (error) {
    
}




}

module.exports  = {
    authenticateUser
}